pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'


    stages {
        stage('Build') {
            steps {
                sh echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                sh echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                sh echo 'Deploying....'

            }
        }
    }
}
